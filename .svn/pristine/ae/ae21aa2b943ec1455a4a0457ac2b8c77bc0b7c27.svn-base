    function xxdbqh(n) {
        var tag = document.getElementById("xxdb").getElementsByTagName("li");
        var taglength = tag.length;

        for (i = 1; i <= taglength; i++) {
            document.getElementById("mx" + i).className = "";
            document.getElementById("zxm" + i).style.display = 'none';
        }
        document.getElementById("mx" + n).className = "xxdb-on";
        document.getElementById("zxm" + n).style.display = '';
        // 判断表格是否存在滚动条,并设置相应的样式
        JudgeTableIsExistScroll();
    }

    function ClickSaveCustomerFinalCheck() {
        var selval = jQuery("input[name='CustomerFinalCheckState']:checked").val();
        if (selval != 1 && selval != 0) {

            art.dialog({
                id: 'artDialogID',
                lock: true,
                fixed: true,
                opacity: 0.3,
                content: '请选择审核意见！',
                button: [{
                    name: '确定',
                    callback: function () {
                        jQuery("#txtCustomerID").focus();  // 设置选中文本框中的文字，并获取光标
                    }
                }],
                close: function () {
                    jQuery("#txtCustomerID").focus();  // 设置选中文本框中的文字，并获取光标
                }
            });
        } else {
            SaveCustomerFinalCheck(selval);
        }
    }

    function ChangeFinalCheckRefuseReason(flag) {
        if (flag == 1) {
            jQuery("#txtRefuseReason").attr("disabled", "disabled");
        } else {
            jQuery("#txtRefuseReason").removeAttr("disabled");
        }
    }

</script>



