/**----------------------------------------------全局样式-------------------------------------------------------**/
html, span, img, li, p, ol, ul, body, fieldset, ht1, ht2, ht3, ht4, ht5, ht6, form, div, table, tr, th, td, iframe, blockquote, pre, a, address, big, cite, code, del, em, font, ins, small, strong, var, b, u, i, center, dl, dt, dd, ol, ul, li, fieldset, label, legend
{
    margin: 0;
    padding: 0;
    border: 0;
}

