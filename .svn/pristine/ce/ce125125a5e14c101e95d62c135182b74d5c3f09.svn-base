.quicktable thead, .quicktable thead tr, .quicktable thead th
{
    background-color: #efefef;
    height: 26px;
    line-height: 26px;
}

.quicktable tbody
{
    background-color: #f9f9f9;
}
.quicktable th, .quicktable td
{
    padding: 2px;
    line-height: 20px;
    text-align: left;
    vertical-align: top;
    border: 1px solid #ddd;
}
.quicktable th
{
    font-weight: bold;
}
.quicktable thead th
{
    vertical-align: bottom;
}
.quicktable caption + thead tr:first-child th, .quicktable caption + thead tr:first-child td, .quicktable colgroup + thead tr:first-child th, .quicktable colgroup + thead tr:first-child td, .quicktable thead:first-child tr:first-child th, .quicktable thead:first-child tr:first-child td
{
    border-top: 0;
}
.quicktable tbody + tbody
{
    border-top: 2px solid #ddd;
}
.quicktable .table
{
    background-color: #fff;
}
.quicktable input
{
    clear:both;
    width:30px;
    border:0px;
} 

