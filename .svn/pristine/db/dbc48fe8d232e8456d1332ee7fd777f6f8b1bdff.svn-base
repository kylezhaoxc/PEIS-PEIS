        /// </summary>
        function ConclusionClickBackground(ID_Conclusion) {

            jQuery("#QueryConclusionDataTableList tr").removeClass();
            jQuery("#trConclusion_" + ID_Conclusion).addClass("SelectedItem");
        }

        function xxdbqh(n) {
            var tag = document.getElementById("xxdb").getElementsByTagName("li");
            var taglength = tag.length;

            for (i = 1; i <= taglength; i++) {
                document.getElementById("mx" + i).className = "";
                document.getElementById("zxm" + i).style.display = 'none';
            }
            document.getElementById("mx" + n).className = "xxdb-on";
            document.getElementById("zxm" + n).style.display = '';

            // 判断表格是否存在滚动条,并设置相应的样式
            JudgeTableIsExistScroll();
        }

        function SetDiagnoseTypeValue(obj){
            var conclusionid =  jQuery(obj).attr("conclusionid");
            
            jQuery("#trSel_" + conclusionid + " span").removeClass();
            jQuery("#trSel_" + conclusionid + " span").addClass(GetFinalConclusionTypeClass(jQuery(obj).val()));
            jQuery("#txtFinalConclusionSignCode_" + conclusionid).val(jQuery(obj).val());
            jQuery("#chkSelectedConclusionList_" + conclusionid).attr("FinalConclusionSignCode", jQuery(obj).val());

        }

