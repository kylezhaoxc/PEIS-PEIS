    <table style="display:none;">
        <!-- 收费项目模版 -->
        <tbody id="FeeDataListTemplete">
            <tr name="trFee_@ID_Fee" style="cursor:pointer;" onclick="ShowHideFeeExamItem('@ID_Fee');">
                <td colspan="2" class="FeeName_title">
                    <div class="fenk-bs-tips">
                        <div class="fenk-bs-tips-2"><lable FeeName="@FeeName" id="lab_@ID_Fee" CustFeeID="@ID_CustFee">@FeeName</lable></div>
                        <div class="fenk-bs-tips-3 j-fenkTips" id="ShowHideFee_@ID_Fee"></div>
                    </div>
                </td>
            </tr>
        </tbody>
        <!-- 体检项目模版 -->
        <tbody id="ExamDataListTemplete">
            <tr name="trExamFee_@ID_Fee" class="fenk-td1-@BGStyleNo">
                <td class="fenk-bs-xqlb-table-td@BGStyleNo" nowrap="nowrap"><lable IsEntrySectSum="@Is_EntrySectSum" id="lab_@ID_ExamItem" ExamItemName="@ExamItemName">@ExamItemName：</lable></td>
                <td class="fenk-bs-xqlb-table-td3">
                    @ExamItemSymptomAndResultDivInfo
                </td>
            </tr>
        </tbody>

        <!-- 检查医生模版 -->
        <tbody id="ExamDoctorSelectTemplete">
            <tr name="trExamFee_@ID_Fee"  >
                <td class="examitemleft submitlinestyle" nowrap="nowrap" style="line-height:30px;">检查医生：</td>
                <td style="text-align:left;">
                    &nbsp;&nbsp;
                    <input type="hidden" id="ExamDoctor_@FeeItemID" name="txtExamDoctor" value="@ExamDoctorID" DoctorName="@ExamDoctorName" />
                    <span id="Lab_ExamDoctor_@FeeItemID" name="spanExamDoctor">@ExamDoctorName</span>
                    &nbsp;&nbsp;&nbsp;&nbsp;
                    检查时间：&nbsp;&nbsp;<span name="Lab_ExamDate">$!{today}</span>&nbsp;&nbsp;&nbsp;&nbsp;
                </td>
            </tr>
        </tbody>

        <!-- 操作按钮模版 -->
        <tbody id="ExamOperatorButtonTemplete">
            <tr style="display:none;">
                <td class="textright submitlinestyle">&nbsp;</td>
                <td colspan="3"> 

                </td>
            </tr>
        </tbody>
        

        <!-- 体检项目各个模块定义的子模版 -->
        <tbody>
            <tr>
                <td colspan="2">

                    <!-- 文本输入框模版 -->
                    <div id="InputTextTemplete">
                        <div class="ClassSymptomCols_@SymCols">
                            <input class="ClassSymtomTextInput" @CalcExpression type="text" ExamItemID="@ExamItemID" name="txtSym_@InputTextID" id="txtSym_@InputTextID" value="" /><span id="txtSymUnit_@InputTextID">@InputTextExamItemUnit</span>
                        </div>
                    </div>
                    <!-- 自动计算数据模版 -->
                    <div id="AutoCalculTemplete">
                        <div style="display:none;"><input type="radio" @Is_Checked  Is_Default='@Is_Default' id="AutoCalcul_@AutoCalculID" name="AutoCalcul_@InputTextID" NumOperSign="@NumOperSign" AutoCalculNum="@AutoCalculNum" SymptomName="@SymptomName" value="@AutoCalculValue" DiseaseLevel="@DiseaseLevel"/>&nbsp;@SymptomName</div>
                    </div>

                    <!-- 单选按钮模版 -->
                    <div id="RadioTemplete">
                        <div class="ClassSymptomCols_@SymCols" id="SymptomDiv_@SymptomRadioID">
                            <input class="ClassSymptomRadio" type="radio" name="radioSym_@SymptomRadioParentID" id="radioSym_@SymptomRadioID" value="@RadioValue" /><label for="radioSym_@SymptomRadioID" id="lab_@SymptomRadioID" DiseaseLevel="@DiseaseLevel" SymptomName="@SymptomRadioShowLabel" SymptomDescribe="@SymptomDescribe">@SymptomRadioShowLabel</label>
                        </div>
                    </div>
                    <!-- 复选按钮模版 -->
