    <input type="hidden" name="TagPrintRelSectionID" id="TagPrintRelSectionID" value="$!{TagPrintRelSectionID}" />
	<!-- 打印标签对应的模版 -->
    <input type="hidden" name="TagPrintTemplete" id="TagPrintTemplete" value="$!{TagPrintTemplete}" />
	<!-- 是否打印当前科室的标签 -->
    <input type="hidden" name="IsPrintCurrSectionTag" id="IsPrintCurrSectionTag" value="${IsPrintCurrSectionTag}" />
    
	<!-- 是否弃检 -->
    <input type="hidden" name="Is_GiveUp" id="Is_GiveUp" value="0" />
	<!-- 总检查次数 -->
    <input type="hidden" name="totalExamNumber" id="totalExamNumber" value="$!{totalExamNumber}" />
	<!-- 隐藏的身份证号码 -->
    <input type="hidden" name="HiddenIDCardNo" id="HiddenIDCardNo" value="$!{IDCardNo}" />
	<!-- 隐藏的客户体检号，在分科数据对比时使用 -->
    <input type="hidden" name="HiddenCustomerID" id="HiddenCustomerID" value="$!{txtCustomerID}" />
	<!-- 隐藏的客户姓名，在分科数据对比时使用 -->
    <input type="hidden" name="HiddenCustomerName" id="HiddenCustomerName" value="$!{CustomerName}" />
	<!-- 记录是否自动完成默认，汇总，保存，及提交 -->
    <input type="hidden" name="IsAutoFinishSaveAndSubmit" id="IsAutoFinishSaveAndSubmit" value="0" />
	<!-- 记录需要隐藏的体征词对应的ID -->
    <input type="hidden" name="HiddenSymptomDivID" id="HiddenSymptomDivID" value="" />
	<!-- 当前体检号对应的图像上传服务器地址 -->
    <input type="hidden" name="CurrImageFileUploadUrl" id="CurrImageFileUploadUrl" value="$!{CurrImageFileUploadUrl}" />

	<!-- 标记是否已经显示过图片了 -->
    <input type="hidden" name="Is_ShowUploadImageFile" id="Is_ShowUploadImageFile" value="0" />
	<!-- 是否显示设备上传上来的图像 -->
    <input type="hidden" name="Is_ShowDeviceResultImagesList" id="Is_ShowDeviceResultImagesList" value="$!{IsShowDeviceResultImagesList}" />

	<!-- 是否已经获取采集列表 --> 
    <input type="hidden" name="IsGetCaptureImageList" id="IsGetCaptureImageList" value="0" />
	<!-- 是否显示视频信息 -->
    <input type="hidden" name="IsShowCaptureImage" id="IsShowCaptureImage" value="0" />
	<!-- 判断当前科室是否需要显示采集卡控件 -->
    <input type="hidden" name="IsSectionShowVideoCapture" id="IsSectionShowVideoCapture" value="$!{IsSectionShowVideoCapture}" />

    
	<!-- 是否仅查询当前科室的医生 -->
    <input type="hidden" name="IsShowCurrSectionDoctor" id="IsShowCurrSectionDoctor" value="1" />

	<!-- 判断当前科室是否显示Com口数据读取控件 -->
    <input type="hidden" name="IsSectionShowComRead" id="IsSectionShowComRead" value="$!{IsSectionShowComRead}" />
    <div style="display:none;">
        <ul id="WaitingUploadFileList">
            
        </ul>
    </div>

	<!-- 【客户端】客户体检号对应文件存放路径 -->
    <input type="hidden" name="CustomerFileSavePath" id="CustomerFileSavePath" value="$!{CustomerFileSavePath}" />
	<!-- 【客户端】客户体检号获取结果标记文件存放路径  -->
    <input type="hidden" name="CustomerFileResultFlagSavePath" id="CustomerFileResultFlagSavePath" value="$!{CustomerFileResultFlagSavePath}" />
	<!-- 是否显示 生成本地体检号对应文件的按钮 -->
    <input type="hidden" name="IsShowWriteCustomerFileSectionBtn" id="IsShowWriteCustomerFileSectionBtn" value="$!{IsShowWriteCustomerFileSectionBtn}" />

</body>

</html>
