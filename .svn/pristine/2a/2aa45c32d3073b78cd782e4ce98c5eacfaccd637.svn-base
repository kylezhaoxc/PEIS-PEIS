<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>$!{webName} $!{pageTitle}</title>
    <link href="$!{templatepath}/css/Css_TeamOper.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" language="javascript">
        var UserID = "$!{UserID}"; //登录人ID
        var UserName = "$!{UserName}"; //登录人名称
        var CurDate = "$!{CurDate}"; //当前服务器日期
        var type = "$!{type}"; //当前操作类型
        var modelName = "$!{modelName}"; //当前操作模块
    </script>
   <script src="$!{templatepath}/js/skin05/config/base1.js"></script>
    <script src="$!{templatepath}/js/JS_DiseaseLevelWorkLoad.js" type="text/javascript"></script>
</head>
<body>
    <div class="center">
        <div id="divHidTemplate" style="display: none;">
            <!--检查数量统计模板 Begin-->
            <table id="DiseaseLevelWorkLoadTemplate">
                <tbody>
                    <tr>
                        <td>
                            @index
                        </td>
                        <td>
                            @ID_Customer
                        </td>
                        <td class="project-align-left" title="@CustomerName">
                            <div class="nowrap" style="width: 100%;">
                                @CustomerName</div>
                        </td>
                        <td>
                            @GenderName
                        </td>
                        <td style="display: none;">
                            @SectionName
                        </td>
                        <td style="display: none;">
                            @FeeItemName
                        </td>
                        <td class="project-align-left" title="@ExamItemName">
                            <div class="nowrap" style="width: 100%;">
                                @ExamItemName</div>
                        </td>
                        <td class="project-align-left" title="@ResultSummary">
                            <div class="nowrap" style="width: 100%;">
                                @ResultSummary</div>
                        </td>
                        <td>
                            @ExamItemSummaryDate
                        </td>
                        <td>
                            @DiseaseLevel
                        </td>
                        <td class="project-align-left" title="@SummaryDoctorName">
                            <div class="nowrap" style="width: 100%;">
                                @SummaryDoctorName</div>
                        </td>
                        <td>
                            @Is_Informed
                        </td>
                        <td class="project-align-left" title="@Informer">
                            <div class="nowrap" style="width: 100%;">
                                @Informer</div>
                        </td>
                        <td>
                            @InformedDate
                        </td>
                    </tr>
                </tbody>
            </table>
            <!--检查数量统计模板 End-->
            $!{HiddenInputQueryString}
        </div>
        <div class="operarea">
            <div class="oper-left">
            </div> <div class="oper-right">
            </div><div class="oper-bg1">
                <span>病症级别：</span> <span class="span-margin-top">
                    <div class="Editor-l">
                    </div>
                    <div class="Editor-c">
                        <input type="text" id="txtMinLevel" class="input-1" />
                    </div>
                    <div class="Editor-r">
                    </div>
                </span><span>&nbsp;至&nbsp;</span> <span class="span-margin-top">
                    <div class="Editor-l">
                    </div>
                    <div class="Editor-c">
                        <input type="text" id="txtMiaxLevel" class="input-1" />
                    </div>
                    <div class="Editor-r">
                    </div>
                </span>
                <div id="divInformed">
                    <span>&nbsp;状态：</span> <span class="span-margin-top">
                        <div class="Editor-l">
                        </div>
                        <div class="Editor-c">
                            <select id="slInformed">
                                <option value="-1">--请选择--</option>
                                <option value="0">未通知</option>
                                <option value="1">已通知</option>
                            </select>
                        </div>
                        <div class="Editor-r">
                        </div>
                    </span>
                </div>
                <span>&nbsp;日期：</span> <span class="span-margin-top">
                    <div class="Editor-l">
                    </div>
                    <div class="Editor-c">
                        <input type="text" id="BeginExamDate" name="BeginExamDate" value="${today}" class="datemaxselect"
                            style="width: 75px;" />
                    </div>
                    <div class="Editor-r">
                    </div>
                </span><span>&nbsp;至&nbsp;</span> <span class="span-margin-top">
                    <div class="Editor-l">
                    </div>
                    <div class="Editor-c">
                        <input type="text" id="EndExamDate" name="EndExamDate" value="${today}" class="datemaxselect"
                            style="width: 75px;" />
                    </div>
                    <div class="Editor-r">
                    </div>
                </span>
            </div>
            <div class="oper-bg">
                <span class="buttom-dc float-right" title="导出"><a href="javascript:void(0);"
                    id="btnExport" name="btnExport" onclick="OutToExcel();">导出</a></span> <span class="buttom-cx-s float-right"
                        title="查询(F4)"><a href="javascript:void(0);" id="btnSearch" name="btnSearch" onclick="GetDiseaseLevelWorkLoad();">
                            查询</a></span><a id="loadExcel" style="display: none;" target="_blank">导出</a>
            </div>
           
        </div>
        <div class="project">
            <div class="project-center" id="bzz">
                <div>
                    <table width="100%" border="0" cellspacing="0" cellpadding="0" class="stripe">
                        <colgroup>
                            <col style="width: 45px" />
                            <col style="width: 11%" />
                            <col style="width: 6%" />
                            <col style="width: 4%" /> 
                            <col style="width:15%" />

                            <col style="width: 13%" />
                            <col style="width: 13%" />
                            <col style="width: 4%" />
                            <col style="width: 6%" />
                            <col style="width: 5%" /> 
                            
                            <col style="width: 6%" /> 
                        </colgroup>
                        <tr id="project-center-lb1-list">
                            <th>
                                序号
                            </th>
                            <th>
                                体检号
                            </th>
                            <th>
                                姓名
                            </th>
                            <th>
                                性别
                            </th>
                            <th>
                                检查项目
                            </th>
                            <th>
                                检查结果
                            </th>
                            <th>
                                检查时间
                            </th>
                            <th>
                                级别
                            </th>
                            <th>
                                检查人
                            </th>
                            <th>
                                状态
                            </th>
                            <th>
                                通知人
                            </th>
                            <th>
                                通知时间
                            </th>
                        </tr>
                    </table>
                </div>
                <div id="QueryExamListData" class="j-autoHeight QueryListNoDataTips" data-left="330"
                    data-min="200" style="overflow: auto;">
                    <table id="tbDiseaseLevel" width="100%" border="0" cellspacing="0" cellpadding="0"
                        class="stripe">
                        <colgroup>
                            <col style="width: 45px" />
                            <col style="width: 11%" />
                            <col style="width: 6%" />
                            <col style="width: 4%" /> 
                            <col style="width:15%" />

                            <col style="width: 13%" />
                            <col style="width: 13%" />
                            <col style="width: 4%" />
                            <col style="width: 6%" />
                            <col style="width: 5%" /> 

                            <col style="width: 6%" /> 
                        </colgroup>
                        <tbody id="Searchresult">
                        </tbody>
                    </table>
                </div>
                <table width="100%" border="0" cellspacing="0" cellpadding="0" id="project-center-lb1-list-bottom">
                    <tr>
                        <td colspan="10">
                            <div class="paging" id="Pagination">
                                <div class="paging-left">
                                </div>
                                <div class="paging-center">
                                    <ul>
                                    </ul>
                                </div>
                                <div class="paging-right">
                                </div>
                            </div>
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <div id="divLoadCustomerReport" style="display: none;">
        </div>
    </div>
</body>
</html>
