        var teamID = "$!{ID_Team}";
        var Base64PhtoStr = "$!{Base64Photo}";
        var Code128c = "$!{Code128c}";
        var UserID = "$!{UserID}"; //登录人名称
        var UserName = "$!{UserName}"; //登录人名称
        /************************************************/
