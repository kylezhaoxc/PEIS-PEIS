<script type="text/javascript">

    /// <summary>
    /// 隐藏快速查询框
    /// </summary>
    function HideAllQuickTable() {
        ShowHideQuickQueryTeamTable(false, "");      // 团体
        ShowHideQuickQueryTeamTaskTable(false, "");  // 团体任务
    }
    //团体点击时重置
    function TeamCallBack() {
        ResetInfo();
    }
    //点击团体任务时
    function TeamTaskCallBack() {

        ResetInfo();
        var TeamID = jQuery('#idSelectTeam').val();
        var TeamTaskID = jQuery('#idSelectTeamTask').val();
        if (TeamID != -1) {
            ResetInfo();
            //这里绑定所有的团体名单信息
            LoadTeamTaskGroupCustomerInfo(TeamID, TeamTaskID, "tblTeamTaskGroupCustomerX");
        }
        else {
            BindTeamTaskGroup(null);
        }
    }

    /*待收金额 鼠标提示*/
    var timer, startTimer;
    function MouseoverWaitCharge() {
        jQuery(".IncomeX,.come_looseX").unbind();
        jQuery(".IncomeX,.come_looseX").bind("mouseover", function () {
            if (startTimer) { clearTimeout(timer); return false; }
            $(".come_looseX").show();
        });
        jQuery(".IncomeX,.come_looseX").bind("mouseout", function () {
            startTimer = true;
            timer = setTimeout(function () {
                $(".come_looseX").hide();
                startTimer = false;
            }, 300);
        });
    }
    jQuery(document).ready(function () {
        MouseoverWaitCharge();
    });
</script>
