QuickQueryList
{
    width:450px; 
    position:absolute; 
    border:1px solid #ccc; 
    background-color:White; 
    margin-top:-8px;  
    display:none;
    }


.QuickQueryListHeader
{
    color: white;
    width:100%;
    height:24px !important; 
    line-height:24px !important;
    /*background:url(/template/blue2/images/zjxz-jlc-nr-left-table-bg.jpg) top repeat-x;*/
    }
.QuickQueryListHeader th
{
    border-right:1px solid #f1f1f1;
    border-bottom:1px solid #f1f1f1;
    text-align:left;
    padding-left:5px !important;
    }
.QuickQueryListContent
{
    line-height:24px !important;
    }
.QuickQueryListContent td
{
    border-right:1px solid #f1f1f1;
    border-bottom:1px solid #f1f1f1;
    padding-left:5px !important;
    text-align:left;/*xmhuang 新增快速检索td内容为居中显示*/
    }
    
.QuickQueryListButton
{
     width:100%; height:30px; line-height:30px; text-align:center; background-color:#fafafa;
     
    }


/*表格*/
.stripe tr td
{ 
	padding-top:3px;
	padding-bottom:3px;
	height:20px; 
	line-height:20px; 
	text-align:center;
	background:#fbfeff;
	vertical-align:middle;
	}/* 默认背景被白色 */
.stripe tr.alt td 
{ 
	background-color:#e5efff;
	}/* 默认隔行背景颜色 */
.stripe tr.over td
{	
	background-color:#c4dbff;
	}/* 鼠标经过时候背景颜色 */
.stripe td,.stripe th
{
	border-right:1px solid #abccff;
	border-bottom:1px solid #abccff;
	text-align:center;
	background:url(../images/stripe-th-bg.jpg) center repeat-x;
	}

.gudingheight {
    height:400px;
    overflow:auto;
    margin-right: -20px;   
}