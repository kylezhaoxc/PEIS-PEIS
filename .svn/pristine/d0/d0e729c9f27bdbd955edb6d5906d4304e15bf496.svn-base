/** 进度条样式 **/
.loading
{
    z-index:999;
}
.loading .frame
{
    background-image:url('../images/loading_bg2.png');
    width:206px;
    height:131px;
    z-index:100;
    position:absolute; 
    top:10px;
    left:10px;
    display:none;
}
.loading .frame .title
{
    /*font-size:12px;*/
    color:#999999;
    line-height:23px;
    height:23px;
    width:160px;
    padding-top:15px;
    padding-left:25px;
}

.loading .frame .info
{
    font-size:10px;
    color:#999999;
    line-height:23px;
    height:23px;
    width:160px;
    padding-top:10px;
    padding-left:25px;
    text-align:center;
}
.loading .frame .Process
{
    padding-left:30px;
    width:160px;
    text-align:center;
}
.loading .frame .Process .Bar
{
    background-image:url('../images/loading_4.gif');
    height:16px;
    width:139px;
}

.nav-center .nav{
	padding-left:20px;
	list-style:none;
	margin:0px;
	padding:0px;
}
.nav-center .nav li{
	float:left;
	height:36px;
}
.nav-center .nav li a{
	float:left;
	display:block;
	padding-left:5px;
	height:36px;
	background:url(../images/menu_on_bg.gif) repeat-x left top;
	cursor:pointer;
	text-decoration:none;
}
.nav-center .nav li a span{
	float:left;
	padding:10px 20px 7px 20px;
	line-height:14px;
	font-size:14px;
	font-weight:bold;
	color:#FFFFFF;
	text-decoration:none;
}
.nav-center .nav li .nav-on{   /*鼠标点击时变换背景，方便JS获取样式*/
	background-position:left 100%;
}
.nav-center .nav li .nav-on span{  /*鼠标点击时变换背景，方便JS获取样式*/
	background-position:right 100%;
	color:#fff;
	text-decoration:none;
	padding:10px 24px 7px 20px;
}

