function showUserRightLeftList(userRightLeftList) {
    //创建菜单  userLogin  登陆用户
    var userLoginSecondMenu = userRightLeftList.dataList0; //二级菜单
    var userLoginRightJsonData = userRightLeftList.dataList1; // 登录用户的权限变量（二级菜单）
    var userLoginSection = userRightLeftList.dataList2;   // 用户具有的科室
    var userLoginMenuClass = userRightLeftList.dataList3; // 用户主菜单
    //分别写入各个数据
    var userLoginLeftFirstList = "";//用户一级菜单
    //var userLoginLeftSecondList = "";//用户一级菜单
    $.each(userLoginMenuClass, function (k, firstMenuName) {
        if (firstMenuName.MenuName === "登记系统") {
            var iconName = "icon-file-text";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "财务管理") {
            var iconName = "icon-rmb";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "常规检查") {
            var iconName = "icon-user-md";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "检验检查") {
            var iconName = "icon-stethoscope";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "影像检查") {
            var iconName = "icon-video-camera";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "总检系统") {
            var iconName = "icon-h-square";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "报告管理") {
            var iconName = "icon-file-word-o";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "报表系统") {
            var iconName = "icon-table";
            var hhColorName = "bg-green";
        } else if (firstMenuName.MenuName === "系统设置") {
            var iconName = "icon-cog";
            var hhColorName = "bg-green";
        }

        userLoginLeftFirstList += '<h2 class=""><span class="' + iconName + '" aria-hidden="true"></span>' + firstMenuName.MenuName + '</h2><ul>';//填写一级菜单
        $.each(userLoginSecondMenu, function (j, secondMenuName) {
            //查找二级菜单等于一级的id
            if (firstMenuName.MenuID === secondMenuName.ParentID) {
                /*判断属性*/
                if (secondMenuName.Is_CombineWithSection === "True") {
                    /*如果是true*/
                    $.each(userLoginSection, function (i, sectionItems) {
                        if (secondMenuName.MenuName === sectionItems.DisplayMenu) {
                            userLoginLeftFirstList += '<li><a  dizhi="' + secondMenuName.Url + '?txtSectionID=' + sectionItems.SectionID + '" class="secondList" target="right"><span class="icon-caret-right"></span>' + sectionItems.SectionName + '</a></li>';
                        }
                    })
                } else {
                    userLoginLeftFirstList += '<li><a  dizhi="' + secondMenuName.Url + '" class="secondList" target="right"><span class="icon-caret-right"></span>' + secondMenuName.MenuName + '</a></li>';
                }
            }

        });
        userLoginLeftFirstList += '</ul>'
    });
    $(".leftnav").html(userLoginLeftFirstList);
    $(".leftnav h2").click(function () {
        $(this).next().toggle(200);
        $(".leftnav h2").removeClass("on");
        $(this).addClass("on");
        $(".leftnav h2[class!='on']").next().hide(200);
    });
    $(".leftnav ul li a").click(function () {
        //$("#a_leader_txt").text($(this).text());
        var href = $(this).attr("dizhi");
        $("#iframe").attr("src", href);
        $(".leftnav ul li a").removeClass("on");
        $(this).addClass("on");
        //此函数执行完才会跳转
        //intoJs()
    });
    
    
}
