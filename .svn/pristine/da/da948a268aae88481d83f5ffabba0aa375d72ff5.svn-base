<script type="text/javascript" language="javascript">
    /// <summary>
    /// 隐藏快速查询框
    /// </summary>
    function HideAllQuickTable() {
        ShowHideQuickQueryTeamTable(false, "");      // 团体
        ShowHideQuickQueryTeamTaskTable(false, "");  // 团体任务
    }
    //团体点击时重置
    function TeamCallBack() {
        ResetInfo();
    }
    /*待收金额 鼠标提示*/
    var timer, startTimer;
    function MouseoverWaitCharge() {
        jQuery(".Income,.come_loose").unbind();
        jQuery(".Income,.come_loose").bind("mouseover", function () {
            if (startTimer) { clearTimeout(timer); return false; }
            $(".come_loose").show();
        });
        jQuery(".Income,.come_loose").bind("mouseout", function () {
            startTimer = true;
            timer = setTimeout(function () {
                $(".come_loose").hide();
                startTimer = false;
            }, 300);
        });
    }
    jQuery(document).ready(function () {
        MouseoverWaitCharge();

    });
    //关闭名单列表
    function OpenOrCloseCustomerList() {
        CommonShowHideDialog('showDialog');
        //关闭团体客户名单窗口 xmhuang 2014-04-18
        jQuery("#TeamCustomerAddDiv").hide();
    }
    //关闭团体名单编辑窗口
    function CloseTeamAddDialog() {
