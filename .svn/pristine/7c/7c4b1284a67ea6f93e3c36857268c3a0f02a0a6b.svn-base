	<!-- 隐藏的客户姓名，在数据对比时使用 -->
    <input type="hidden" name="HiddenCustomerName" id="HiddenCustomerName" value="$!{CustomerName}" />
	<!-- 是否加载体征词对应的结论词 -->
    <input type="hidden" name="IsLoadConnectConclusion" id="IsLoadConnectConclusion" value="0" />
        
