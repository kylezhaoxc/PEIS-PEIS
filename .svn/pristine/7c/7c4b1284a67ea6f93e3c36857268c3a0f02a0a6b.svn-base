	//分科信息、历史对比TAB
	function dxqh(n) 
	{
	  var tag = document.getElementById("fkls").getElementsByTagName("li");
	  var taglength = tag.length;
	
	  for (i=1;i<=taglength;i++)
	  {
		document.getElementById("fk"+i).className="";
		document.getElementById("cen"+i).style.display='none';
	  }
		document.getElementById("fk"+n).className="fkls-on";
		document.getElementById("cen"+n).style.display='';
	}
	
	//对比TAB
	function xxdbqh(n) 
	{
	  var tag = document.getElementById("xxdb").getElementsByTagName("li");
	  var taglength = tag.length;
	
	  for (i=1;i<=taglength;i++)
	  {
		document.getElementById("mx"+i).className="";
		document.getElementById("zxm"+i).style.display='none';
	  }
		document.getElementById("mx"+n).className="xxdb-on";
		document.getElementById("zxm"+n).style.display='';
	}
		//预览页面竖向tab切换
			function gopage(n) 
			{
			  var tag = document.getElementById("ylhd").getElementsByTagName("li");
			  var taglength = tag.length;
			
			  for (i=1;i<=taglength;i++)
			  {
				document.getElementById("m"+i).className="";
				document.getElementById("c"+i).style.display='none';
			  }
				document.getElementById("m"+n).className="yl-zjxx-left-a";
				document.getElementById("c"+n).style.display='';
			}

	function qiehuan(num){
		for(var id = 0;id<=10;id++)
		{
			if(id==num)
			{
				document.getElementById("qh-con"+id).style.display="block";
				document.getElementById("mynav"+id).className="nav-on";
			}
			else
			{
				document.getElementById("qh-con"+id).style.display="none";
				document.getElementById("mynav"+id).className="";
			}
		}
	}
	//二级菜单焦点
	function mytabk(ecd) {
	    for (var id = 0; id <= 115; id++) {
	        if (id == ecd) {
	            document.getElementById("tabk-qh" + id).style.backgroundImage = "url(images/menu_ho.gif)";
	            document.getElementById("tabk-qh" + id).style.color = "#fff";
	        }
	        else {
	            document.getElementById("tabk-qh" + id).style.background = "";
	            document.getElementById("tabk-qh" + id).style.color = "";
	        }
	    }
	}
</script>

