<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Transitional//EN" "http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd">
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>$!{webName} $!{pageTitle}</title>
    <link href="$!{templatepath}/css/Css_TeamOper.css" rel="stylesheet" type="text/css" />
    <script type="text/javascript" language="javascript">
        var UserName = "${UserName}"; //登记人
        var ID_User = "${ID_User}"; //登记人
        var CurDate = "${CurDate}"; //当前服务器时间
        var RegisteDate = "${RegisteDate}"; //登记日期
        var Register = "${Register}"; //登记人
        var DisCountRate = "$!{DisCountRate}"; //折扣为0表示没有折扣
        DisCountRate = (DisCountRate == "" || DisCountRate == 0) ? 10 : DisCountRate;
    </script>
    <script src="$!{templatepath}/js/JS_TeamSearch.js" type="text/javascript"></script>
    <script src="$!{templatepath}/js/JS_TeamBatch.js" type="text/javascript"></script>
</head>
<body>
    <form onkeyup="OnFormKeyUp();">
    <div style="display: none;">
        <!--团体检索模版 Begin-->
        <table id="tblTemplateTeamList">
            <thead>
                <tr>
                    <td>
                        @TeamNameTitle
                    </td>
                    <td>
                        @InputCodeTitle
                    </td>
                </tr>
            </thead>
            <tbody>
                <tr id="@ID_Team">
                    <td name="show">
                        @TeamName
                    </td>
                    <td>
                        @InputCode
                    </td>
                </tr>
            </tbody>
        </table>
        <!--团体检索模版 End-->
        <!--团体任务检索模版 Begin-->
        <table id="tblSearchTemplateTeamTaskList">
            <thead>
                <tr>
                    <td>
                        @TeamNameTitle
                    </td>
                    <td>
                        @InputCodeTitle
                    </td>
                </tr>
            </thead>
            <tbody>
                <tr id="@ID_Team">
                    <td name="show">
                        @TeamName
                    </td>
                    <td>
                        @InputCode
                    </td>
                </tr>
            </tbody>
        </table>
        <!--团体任务检索模版 End-->
        <!--团体任务分组套餐模版 Begin-->
        <table id="tblTemplateTeamTaskGroupFee">
            <thead>
                <tr>
                    <td>
                        <input type="checkbox" onclick="CheckAllTeamTaskGroupData(this,'tblTeamTaskGroupFee');" />
                    </td>
                    <td>
                        所属分组
                    </td>
                    <td>
                        序号
                    </td>
                    <td>
                        项目名称
                    </td>
                    <td>
                        原价(元)
                    </td>
                    <td>
                        折扣
                    </td>
                    <td>
                        实价(元)
                    </td>
                    <td>
                        付费方式
                    </td>
                </tr>
            </thead>
            <tbody>
                <tr class="@class" id_set="@PEPackageID" id_teamtaskgroup="@ID_TeamTaskGroup" exist="@exist"
                    custfeechargestate="@CustFeeChargeState" name="busList" id="@ID_Fee" id_fee="@ID_Fee"
                    id_section="@ID_Section" sectionname="@SectionName">
                    <td style="display: none;">
                        <input type="checkbox" parentid="@ID_Fee" id="Checkbox_@ID_Fee" name="ItemCheckbox" />
                    </td>
                    <td>
                        <label name="lblRowNum" style="width: 10px;">
                            @RowNum</label>
                    </td>
                    <td>
                        <label name="fzmc">
                            @TeamTaskGroupName</label>
                    </td>
                    <td>
                        <label name="xmmc">
                            @FeeName</label>
                    </td>
                    <td>
                        <label name="yj">
                            @Price</label>
                    </td>
                    <td>
                        <label custfeechargestate="@CustFeeChargeState" name="zk">
                            @Discount</label>
                    </td>
                    <td>
                        <label name="sj">
                            @FactPrice</label>
                    </td>
                    <td name="tdFeeWay">
                        <label exist="@exist" feetype="@ID_FeeType" name="fffs">
                            @FeeWayName</label>
                    </td>
                </tr>
            </tbody>
        </table>
        <!--团体任务分组套餐模版 End-->
        <!--团体名单模版 Begin-->
        <table class="tblRegistTableEdit" id="tblTemplateTeamTaskGroupCustomer">
            <thead>
                <tr>
                    <td class="text_left width20">
                        <input type="checkbox" onclick="CheckAllCustomer(this,'tblTeamTaskGroupCustomerX');" />
                    </td>
                    <td style="width: 10px;">
                        序号
                    </td>
                    <td style="min-width: 60px;">
                        分组名称
                    </td>
                    <td style="min-width: 60px;">
                        体检号
                    </td>
                    <td>
                        姓名
                    </td>
                    <td>
                        身份证
                    </td>
                    <td>
                        性别
                    </td>
                    <td>
                        婚姻状况
                    </td>
                    <td>
                        角色
                    </td>
                    <td>
                        出生日期
                    </td>
                    <td>
                        联系电话
                    </td>
                    <td>
                        部门
                    </td>
                    <td>
                        二级部门
                    </td>
                    <td>
                        三级部门
                    </td>
                    <td>
                        四级部门
                    </td>
                </tr>
            </thead>
            <tbody>
                <tr class="@className" exist="@exist" id_customer="@ID_Customer" flag="@flag" id_teamtaskgroup="@ID_TeamTaskGroup"
                    groupname="@TeamTaskGroupName" errormessage="@ErrorMessage">
                    <td>
                        <input style="width: 18px;" type="checkbox" name="ItemCheckbox" onclick="ChangeCustomerCustInfo(this);" />
                    </td>
                    <td>
                        <label name="lblRowNum" style="width: 10px;">
                            @RowNum</label>
                    </td>
                    <td>
                        <label name="lblGroupName">
                            @TeamTaskGroupName</label>
                    </td>
                    <td>
                        <label name="lblID_Customer">
                            @ID_Customer</label>
                    </td>
                    <td>
                        <label name="txtCustomerName" style="width: 50px;">
                            @CustomerName</label>
                    </td>
                    <td>
                        <label name="txtIDCard" style="width: 50px;">
                            @IDCard</label>
                    </td>
                    <td>
                        <label name="slCustomerSex" style="width: 50px;">
                            @GenderName</label>
                    </td>
                    <td>
                        <label name="slCustomerMarried" style="width: 60px;">
                            @MarriageName</label>
                    </td>
                    <td>
                        <label name="txtCustomerRoleName" style="width: 50px;">
                            @CustomerRoleName</label>
                    </td>
                    <td>
                        <label name="txtCustomerBirthDay" style="width: 100px;">
                            @CustomerBirthDay</label>
                    </td>
                    <td>
                        <label name="txtCustomerTel" style="width: 100px;">
                            @CustomerTel</label>
                    </td>
                    <td>
                        <label name="txtDepartment" style="width: 50px;">
                            @DepartmentX</label>
                    </td>
                    <td>
                        <label name="txtDepartmentA" style="width: 50px;">
                            @DepartmentA</label>
                    </td>
                    <td>
                        <label name="txtDepartmentB" style="width: 50px;">
                            @DepartmentB</label>
                    </td>
                    <td>
                        <label name="txtDepartmentC" style="width: 50px;">
                            @DepartmentC</label>
                    </td>
                </tr>
            </tbody>
        </table>
        <!--团体名单模版 End-->
        <!--团体批量新增、删除套餐模版 Begin-->
        <table id="TemplateTeamBatchSearch">
            <tbody>
                <tr name="trExternItem" class="trExternItem" custfeechargestate="@CustFeeChargeState"
                    id_fee="@ID_Fee" feename="@FeeName" id_set="@PEPackageID" username="@userName" id_section="@ID_Section"
                    price="@Price">
                    <td>
                        <input onkeydown="keyMove(this, event)" name="ItemCheckboxX" type="checkbox">
                    </td>
                    <td>
                        <input readonly="readonly" style="width: auto; border: 0px;" name="textExternItem"
                            onkeydown="keyMove(this, event)" readonly="readonly" value="@FeeName">
                    </td>
                    <td>
                        <input readonly="readonly" style="width: auto; border: 0px;" name="textExternItem"
                            onkeydown="keyMove(this, event)" type="text" readonly="readonly" value="@InputCode">
                    </td>
                    <td>
                        <input readonly="readonly" style="width: auto; border: 0px;" name="textExternItem"
                            onkeydown="keyMove(this, event)" type="text" readonly="readonly" value="@Price">
                    </td>
                </tr>
            </tbody>
        </table>
        <!--团体批量新增、删除套餐模版 End-->
        <select class="select_max" id="hidslBusSet" name="hidslBusSet">
            <option code="qxz" forgender="2" value="-1">----</option>
            #foreach($info in $!{BusPEPackageDT.Rows})
            <option code="${info.InputCode}" forgender="${info.Forsex}" value="${info.PEPackageID}">
                ${info.PEPackageName}</option>
            #end
        </select>
        <select id="hidslFeeWay" name="hidslFeeWay">
            #foreach($info in $!{DictFeeWayDT.Rows}) #if('True'==${info.Is_Default})
            <option code="${info.InputCode}" value="${info.ID_FeeWay}" is_default="${info.Is_Default}">
                ${info.FeeWayName}</option>
            #else
            <option code="${info.InputCode}" value="${info.ID_FeeWay}" is_default="${info.Is_Default}">
                ${info.FeeWayName}</option>
            #end #end
        </select>
        $!{HiddenInputQueryString}
    </div>
    <div class="content">
        <!-- 快速导航区域 根据不同权限显示不同科室 -->
        <div class="widget-box showbox" style="display: none;">
            <div class="divOperate" style="height: 20px; line-height: 20px;">
                <input id="btnBack" type="button" class="button80" value=" 返  回 " onclick="DoLoad('/System/Customer/SignList.aspx','')" />
                <input id="Button1" name="btnAdd" type="button" class="button80" value=" 新增登记 " onclick="AddNewRegistOrSign();" />
                <input id="btnSave" name="btnSave" type="button" class="button80" value=" 保  存 "
                    onclick="return vailData();" />
            </div>
        </div>
        <!--团体、任务信息检索 Begin-->
        <div class="widget-box showbox">
            <div class="widget-title bg_lg widget-title-middle">
                筛选信息
            </div>
            <table class="tblRegistTableEdit" id="tblRegistTableEdit">
                <tr>
                    <td class="text_right">
                        团体名称：
                    </td>
                    <td class="width350">
                        <select id="txtTeamNameX" name="txtTeamNameX" style="width: 300px;" required="Required"
                            errormessage="体检类型不允许为空！">
                            <option code="qzz" value="-1" selected="selected">-请选择团体--</option>
                            #foreach($info in $!{TeamDT.Rows})
                            <option code="${info.InputCode}" value="${info.ID_Team}">${info.TeamName}</option>
                            #end
                        </select>&nbsp;
                    </td>
                    <td class="text_right width100">
                        任务名称：
                    </td>
                    <td colspan="5">
                        <select id="txtTeamTaskNameX" class="select_max" name="txtTeamTaskNameX" style="width: 300px;"
                            required="Required" errormessage="体检类型不允许为空！">
                            <option code="qzz" value="-1" selected="selected">-请选择任务--</option>
                        </select>
                    </td>
                </tr>
                <tr>
                    <td class="text_right">
                        分组名称：
                    </td>
                    <td class="width350">
                        <select id="slTeamTaskGroup" name="slTeamTaskGroup" title="下拉可选择不同的分组进行查询" style="width: 300px;">
                            <option code="qzz" value="-1" selected="selected">-请选择分组--</option>
                        </select>
                    </td>
                    <td class="text_right">
                        体检/证件号(F2)
                    </td>
                    <td>
                        <input type="text" class="span100" id='txtSFZ' placeholder=" " />
                    </td>
                    <td class="text_right">
                        姓名:
                    </td>
                    <td>
                        <input type="text" class="span60" id="txtCustomerName" name="txtCustomerName" placeholder=" " />&nbsp;&nbsp;&nbsp;
                        <input id="btnSearch" type="button" class="button70" value="查 询(F4)" onclick="DoSearch();" />
                    </td>
                </tr>
            </table>
        </div>
        <!--团体、任务信息检索 End-->
        <!--客户名单 Begin-->
        <div class="widget-box showbox">
            <div class="widget-title bg_lg widget-title-middle">
                <div style="float: left;" name="ToggerMe" allowtogger="1" id="divCustomerInfo">
                    客户名单
                </div>
                <label id="Label3" style="color: Red;">
                </label>
            </div>
            <div>
                <div style="overflow: scroll;">
                    <div style="height: 300px;">
                        <table class="tblRegistTableEdit" id="tblTeamTaskGroupCustomerX">
                            <thead>
                                <tr>
                                    <td class="text_center width20">
                                        <input type="checkbox" onclick="CheckAllCustomer(this,'tblTeamTaskGroupCustomerX');" />
                                    </td>
                                    <td style="min-width: 30px;">
                                        序号
                                    </td>
                                    <td style="min-width: 60px;">
                                        分组名称
                                    </td>
                                    <td style="min-width: 60px;">
                                        体检号
                                    </td>
                                    <td style="min-width: 40px;">
                                        姓名
                                    </td>
                                    <td style="min-width: 40px;">
                                        身份证
                                    </td>
                                    <td style="min-width: 40px;">
                                        性别
                                    </td>
                                    <td style="min-width: 60px;">
                                        婚姻状况
                                    </td>
                                    <td style="min-width: 40px;">
                                        角色
                                    </td>
                                    <td style="min-width: 80px;">
                                        出生日期
                                    </td>
                                    <td style="min-width: 60px;">
                                        联系电话
                                    </td>
                                    <td style="min-width: 100px;">
                                        部门
                                    </td>
                                    <td style="min-width: 60px;">
                                        二级部门
                                    </td>
                                    <td style="min-width: 60px;">
                                        三级部门
                                    </td>
                                    <td style="min-width: 60px;">
                                        四级部门
                                    </td>
                                </tr>
                            </thead>
                            <tbody>
                                <tr id="loading">
                                    <td colspan="16" style="text-align: center;">
                                        没有任何数据，请您维护！...
                                    </td>
                                </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
                <div class="pagination" id="PaginationOld">
                    <ul id="Pagination">
                    </ul>
                </div>
            </div>
        </div>
        <!--客户名单 End-->
        <!--体检项目 Begin-->
        <div class="widget-box showbox">
            <div class="widget-title bg_lg widget-title-middle">
                <div id="divSumHeader" name="ToggerMe" allowtogger="1">
                    &nbsp;收费项目
                </div>
            </div>
            <div>
                <div style="min-height: 260px; overflow: scroll; height: 260px;">
                    <table id="tblTeamTaskGroup">
                        <tr>
                            <td colspan="2">
                                <table class="tblExamItemTable" id="tblTeamTaskGroupFee" style="width: 100%;">
                                    <thead>
                                        <tr>
                                            <td style="display: none;">
                                                <input type="checkbox" onclick="CheckAllTeamTaskGroupData(this,'tblTeamTaskGroupFee');" />
                                            </td>
                                            <td>
                                                序号
                                            </td>
                                            <td>
                                                所属分组
                                            </td>
                                            <td>
                                                项目名称
                                            </td>
                                            <td>
                                                原价(元)
                                            </td>
                                            <td>
                                                折扣
                                            </td>
                                            <td>
                                                实价(元)
                                            </td>
                                            <td>
                                                付费方式
                                            </td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr id="loading">
                                            <td colspan="8" style="text-align: center;">
                                                没有任何数据，请您维护！...
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>
                            </td>
                        </tr>
                    </table>
                </div>
                <table id="tblExtern" class="tblExtern">
                    <tr id="showBusFee" style="display: none;">
                        <td colspan="2" style="text-align: left;">
                            <div id="divExternList" style="border: 2px solid Blue; height: 300px; width: 600px;
                                overflow: scroll;">
                                <div id="divExternItem">
                                    <table style="max-height: 200px;" id="externTBlList" class="tblExamItemTable">
                                        <thead>
                                            <tr>
                                                <td style="width: 20px;">
                                                    <input id="chbAll1" name="checkAllX" onkeydown="keyMove(this, event)" onclick="checkAllChildren(this);"
                                                        type="checkbox" />
                                                </td>
                                                <td>
                                                    <input type="text" id="txtSearch" onkeyup="OnKeyUp();" style="width: 99%;" />
                                                </td>
                                                <td>
                                                    <input id="btnSure" name="btnSure" onclick="SureAdd();" type="button" class="button80"
                                                        style="width: 60px;" value=" 确  定 " />
                                                    <input id="btnClose" name="btnClose" onclick="DoClose();" type="button" class="button80"
                                                        style="width: 60px;" value=" 关  闭 " />
                                                </td>
                                            </tr>
                                        </thead>
                                        <tbody id="showBusFeeItem">
                                            <tr name="busList" exist="1">
                                                <td colspan="4">
                                                    正在加载，请稍候...
                                                </td>
                                            </tr>
                                        </tbody>
                                    </table>
                                </div>
                            </div>
                        </td>
                    </tr>
                    <tr>
                        <td colspan="2" class="text_right" style="text-align: right; vertical-align: middle;
                            padding-top: 10px;">
                            登记日期：$!{RegisteDate}; 登记人：$!{Register};
                        </td>
                    </tr>
                </table>
                <div>
                    <div class="divOperate" style="height: 20px; line-height: 20px;">
                        <label id="lblErrorMessage" style="color: Red;">
                        </label>
                        <label>
                            付费方式：</label>
                        <select id="slFeeWay" name="slFeeWay" required="Required" emportmessage="付费方式不允许为空！"
                            errormessage="付费方式不允许为空">
                            #foreach($info in $!{DictFeeWayDT.Rows}) #if('统一收费'==${info.FeeWayName})
                            <option code="${info.InputCode}" value="${info.ID_FeeWay}" is_default="${info.Is_Default}"
                                selected="selected">${info.FeeWayName}</option>
                            #else
                            <option code="${info.InputCode}" value="${info.ID_FeeWay}" is_default="${info.Is_Default}">
                                ${info.FeeWayName}</option>
                            #end #end
                        </select>
                        <label>
                            统一折扣：</label><input type="text" value="10" onkeyup="OnDisCoutChange(this);" class='xmzj'
                                id="txtXMZK" name="txtXMZK" onkeypress="return (/[\d.]/.test(String.fromCharCode(event.keyCode)))" />
                        <input type="button" class="button80" id="btnBatchAdd" value=" 批量新增 " onclick="DoAddX(this);" />
                        <input type="button" class="button80" id="btnBatchDelete" value=" 批量删除 " onclick="DoAddX(this);" />
                        <!--此功能暂不开放 Begin-->
                        <input style="display: none;" type="button" class="button80" id="btnBatchPrint" value=" 批量打印 "
                            onclick="PrintReport();" />
                        <!--此功能暂不开放 End-->
                    </div>
                </div>
            </div>
        </div>
        <!--体检项目 End-->
    </div>
    </form>
</body>
</html>
