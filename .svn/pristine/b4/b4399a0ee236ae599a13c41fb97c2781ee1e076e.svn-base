.quit a,.password a
{
	text-decoration:none;
	color:#222;
	padding:0px 10px 0px 15px;
	}
.quit a:hover,.password a:hover
{
	text-decoration:underline;
	color:#F00;
	}
.time
{
	width:284px;
	height:26px;
	line-height:26px;
	float:right;
	clear:both;
	color:#222;
	padding-left:15px;
	background:url(../images/time-bg.png) no-repeat center;
	_background:none;
    _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src="images/time-bg.png");
	}
.time span
