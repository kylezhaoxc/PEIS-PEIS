.password
{
	width:75px;
	height:25px;
	line-height:25px;
	margin:8px 0px 1px 0px;
	float:right;
	background:url(../images/iconter.png) no-repeat left;
	_background:none;
    _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src="images/iconter.png");
	_line-height:15px;
	_margin:14px 0px 7px 0px;
	}
	
.quit
{
	width:170px;
	height:25px;
	line-height:25px;
	margin:8px 0px 1px 0px;
	padding-left:5px;
	float:right;
	background:url(../images/iconter.png) no-repeat -77px;
	_background:none;
    _filter:progid:DXImageTransform.Microsoft.AlphaImageLoader(src="images/iconter.png");
	_line-height:15px;
	_margin:14px 0px 7px 0px;
