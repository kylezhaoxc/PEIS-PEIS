	}
.nav-left
{
	width:5px;
	height:36px;
	float:left;
	background:url(../images/nav_left.gif) no-repeat center;
	}
.nav-center
{
	min-width:950px;
	height:36px;
	float:left;
	overflow:hidden;
	background:url(../images/nav_bg.gif) repeat-x center;
	}
.nav-right
{
	width:5px;
	height:36px;
	float:right;
	background:url(../images/nav_right.gif) no-repeat center;
	}

.nav-down
{
	width:13px;
	height:13px;
	float:left;
	background:url(../images/menu_down_03.png) no-repeat top;
	}
.nav-down:hover
{
	cursor:pointer;
	background:url(../images/menu_down_03.png) no-repeat bottom ;
	}
.NavDownList
{
	color:#000;
	/*font-size:12px;*/
	position:fixed;
	top:95px;
	right:0px;
	display:none;
	z-index: 900;
}
.NavDownListTop
{
	height:12px;
	position: relative;
}
.NavDownListTop-left
{
	width:12px;
	height:12px;
	float:left;
	background:url(../images/KscenJa.png) top left no-repeat;
}
.NavDownListTop-center
{
	height:12px;
	background:url(../images/KscenSx.png) top center repeat-x;
	position: absolute;
	left: 12px;
	right: 12px;
}
.NavDownListTop-right
{
	width:12px;
	height:12px;
	float:right;
	background:url(../images/KscenJa.png) top right no-repeat;
}
.NavDownListCenter
{
	height:100%;
	max-height:800px;
	overflow: hidden;
}
.NavDownListCenter-left
{
	width:12px;
	height:100%;
	float:left;
	background:url(../images/KscenDc.png) left repeat-y;
}
.NavDownListCenter-center
{
	height:100%;
	float:left;
	background:url(../images/KscenBac.jpg) right bottom no-repeat #b6d8f6;
}
.NavDownListCenter-center ul
{
	list-style:none;
	line-height:30px;
	float: right;
	border-right: 1px solid #98C6F3;
}
.NavDownListCenter-center ul:first-child{
	border-right: none;
}
.NavDownListCenter-center ul li a
{
	display:block;
	color:#000;
	text-decoration:none;
	padding-left:20px;
	padding-right:20px;
}
.NavDownListCenter-center .nav-line
{
    display:none;    
}
.NavDownListCenter-center ul li a:hover,
.NavDownListCenter-center ul li.active a
{
	color:#fff;
	background:#1471cb;
	font-weight:bold;
}

.NavDownListCenter-right
{
	width:12px;
	height:100%;
	float:left;
	background:url(../images/KscenDc.png) right repeat-y;
}
.NavDownListBottom
{
	height:12px;
	position: relative;
}
.NavDownListBottom-left
{
	width:12px;
	height:12px;
	float:left;
	background:url(../images/KscenJa.png) bottom left no-repeat;
}
.NavDownListBottom-center
{
	height:12px;
	position: absolute;
	left: 12px;
	right: 12px;
	background:url(../images/KscenSx.png) bottom center repeat-x;
}
.NavDownListBottom-right
{
	width:12px;
	height:12px;
	float:right;
	background:url(../images/KscenJa.png) bottom right no-repeat;
}

	
	
#nav{
	padding-left:20px;
	list-style:none;
	margin:0px;
	padding:0px;
}
#nav li{
	float:left;
	height:36px;
}
#nav li a{
	float:left;
	display:block;
	padding-left:5px;
	height:36px;
	background:url(../images/menu_on_bg.gif) repeat-x left top;
	cursor:pointer;
	text-decoration:none;
}
#nav li a span{
	float:left;
	padding:10px 13px 10px 8px;
	line-height:14px;
	font-size:14px;
	font-weight:bold;
	color:#FFFFFF;
	text-decoration:none;
}
#nav li .nav-on{   /*鼠标点击时变换背景，方便JS获取样式*/
	background-position:left 100%;
}
#nav li .nav-on span{  /*鼠标点击时变换背景，方便JS获取样式*/
	background-position:right 100%;
	color:#fff;
	text-decoration:none;
	padding:10px 13px 7px 8px;
}
.nav-line{
	background:url(../images/menu_line.gif) no-repeat center top;
	width:2px;
}
.menu-line2{
	background:url(../images/menu_line2.gif) no-repeat center;
	width:12px;
}
#menu-con{
	width:972px;
	padding-left:8px;
	text-align:left;
	/*font-size:12px;*/
	height:100%;
	max-height:155px;
	background:url(../images/menu-bg.jpg) top repeat;
}
.clearfix:after 
{
	clear: both;
	content: '.';
	display: block;
	height: 0px;
	overflow: hidden;
	visibility: hidden;
}
#menu-con ul{
	width:980px;
	text-align:left;
	padding-left:0px;
	clear:both;
	list-style:none;
	margin:0px;
	padding:0px;
}
#menu-con li{
	float:left;
	height:28px;
	padding-top:3px;
}
#menu-con li a{
	display:block;
	float:left;
	background:url(../images/menu_mo.gif) no-repeat left top;
	cursor:pointer;
	color:#333;
	text-decoration:none;
}
#menu-con li a span{
	float:left;
	width:110px;
	text-align:center;
	height:22px;
	line-height:22px;
}
#menu-con li a:hover{
	text-decoration:none;
	color:#fff;
	background:url(../images/menu_ho.gif) no-repeat left bottom;
}
#menu-con li a:hover span{
	background:url(../images/menu_ho.gif) no-repeat right bottom;
}

.nav-li-arrow
{
