#body
{
    width:100%;
	min-width:980px;
	min-height: 100%;
	height: auto !important;
	height: 100%;
	margin: 0 auto -30px;/*margin-bottom的负值等于footer高度*/
	}
#body_to
{
    width:100%;
	min-width:1020px;
	min-height: 100%;
	height: auto !important;
	height: 100%;
	margin: 0 auto -30px;/*margin-bottom的负值等于footer高度*/
	}
.push/*footer赋值高度*/
{
	height: 30px;
	clear: both;
}
.push_to/*footer_to赋值高度*/
{
	height: 30px;
	clear: both;
}
#top
{
    width:100%;
	min-width:980px;
	height:60px;
	text-align:left;
	/*font-size:12px;*/
